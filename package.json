    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@lion/core": "^0.13.6",
    "@lion/form": "^0.7.12",
    "@lion/form-core": "^0.6.14",
    "@lion/input": "^0.10.12",
    "@lion/localize": "^0.15.3",
